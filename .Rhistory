#beta = sociability parameter
get.net <- function (beta, nc=15, h) {
n <- length(beta)
beta_bar <- mean(beta)
links <- vector("list", n)
for (i in 1:(n-1)) {
for (j in (i+1):n) {
if (h[i] != h[j]) { # exclude household members
p_link <- nc * beta[i] * beta[j] / (beta_bar^2 * (n - 1))
if (runif(1) < p_link) {
links[[i]] <- c(links[[i]], j)
links[[j]] <- c(links[[j]], i)
}
}
}
}
return(links)
}
